+
+#ifdef CONFIG_MMC
+static int do_emmc(struct cmd_tbl *cmdtp, int flag, int argc,
+		  char *const argv[])
+{
+	int i;
+	long ret;
+	static int times = 0;
+
+	char cmd[256] = {0};
+	void* addr = (void *)0x88000000;	//Use this address to read write
+	int block = 0x200;
+
+	char buf[128] = "Eswin write test loop time";
+
+	if(argc < 1)
+	{
+		printf("Please input read or write\n");
+		return -1;
+	}
+
+	memset(addr, 0, 512);
+
+	if(memcmp(argv[1], "read", 4) == 0)
+	{
+		sprintf(cmd, "mmc read 0x%p 0x%x 1", addr, block);
+		run_command(cmd, 0);
+	}
+	else
+	{
+		//Read times first
+		sprintf(cmd, "mmc read 0x%p 0x%x 1", addr, block);
+		ret = run_command(cmd, 0);
+
+		*(int *)addr = ++times;
+		printf("Write times: %d\n", times);
+
+		//Write back
+		memcpy((char *)(addr + sizeof(int)), buf, strlen(buf));
+		sprintf(cmd, "mmc write 0x%p 0x%x 1", addr, block);
+		run_command(cmd, 0);
+
+		memset(addr, 0, 512);
+
+		//Read back
+		sprintf(cmd, "mmc read 0x%p 0x%x 1", addr, block);
+		ret = run_command(cmd, 0);
+	}
+
+	printf("\n");
+	for(i=0; i<512; i+=4)
+	{
+		if(i%16 == 0)
+			printf("\n0x%08x\t", i);
+
+		printf("0x%08x ", *(int *)(addr + i));
+	}
+	printf("\n");
+
+	return 0;
+}
+
+#ifdef CONFIG_SYS_LONGHELP
+static char emmc_help_text[] =
+	"- emmc read write function test";
+
+#endif
+
+U_BOOT_CMD(
+	emmc, 2, 0, do_emmc,
+	"do emmc test",
+	emmc_help_text
+);
+#endif
